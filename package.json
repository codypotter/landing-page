{
  "name": "landing-page",
  "version": "0.0.22",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "tag": "npm version patch -m 'Deploy version %s'",
