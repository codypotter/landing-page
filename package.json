    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@parcel/packager-raw-url": "^2.14.4",
    "@parcel/transformer-sass": "^2.14.4",
    "@parcel/transformer-webmanifest": "^2.14.4",
    "bulma": "^1.0.4",
    "parcel": "^2.14.4",
    "sass": "^1.87.0"
  }
}